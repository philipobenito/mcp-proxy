{
    "name": "mcp-proxy",
    "version": "1.0.0",
    "description": "A conductor service that manages and proxies connections to multiple Model Context Protocol (MCP) servers",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "tsx --watch src/index.ts",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "test:coverage": "vitest --coverage",
        "lint": "eslint src/**/*.{js,ts} --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "format:src": "prettier --write src/",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "proxy",
        "conductor",
        "ai",
        "llm"
    ],
    "author": "",
    "license": "MIT",
    "engines": {
        "node": ">=22.0.0",
        "pnpm": ">=9.0.0"
    },
    "packageManager": "pnpm@9.0.0",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.0.0",
        "fastify": "^4.0.0",
        "@fastify/cors": "^9.0.0",
        "@fastify/rate-limit": "^9.0.0",
        "@fastify/websocket": "^10.0.0",
        "ws": "^8.0.0",
        "pino": "^8.0.0",
        "zod": "^3.22.0"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "@types/ws": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "eslint": "^9.0.0",
        "prettier": "^3.0.0",
        "tsx": "^4.0.0",
        "typescript": "^5.6.0",
        "vitest": "^2.0.0",
        "@vitest/coverage-v8": "^2.0.0"
    }
}
